{
  "name": "@switchboard/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "worker": "tsx src/queue/start-worker.ts",
    "worker:start": "node dist/queue/start-worker.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run --dir src",
    "test:integration": "vitest run --dir tests",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.2.0",
    "@fastify/swagger": "^9.7.0",
    "@fastify/swagger-ui": "^5.2.5",
    "@switchboard/ads-spend": "workspace:*",
    "@switchboard/quant-trading": "workspace:*",
    "@switchboard/cartridge-sdk": "workspace:*",
    "@switchboard/core": "workspace:*",
    "@switchboard/db": "workspace:*",
    "@switchboard/schemas": "workspace:*",
    "bullmq": "^5.34.0",
    "fastify": "^5.2.0",
    "fastify-plugin": "^5.1.0",
    "ioredis": "^5.4.0",
    "zod": "^3.23.0",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
